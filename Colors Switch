#include <stdio.h>

int main() {
    char color = 'R';

    switch (color) {
        case 'R':
            printf("Red\n");
            break;
        case 'B':
            printf("Blue\n");
            break;
        case 'Y':
            printf("Yellow\n");
            break;
        default:
            printf("Unknown color\n");
    }
    return 0;
}
